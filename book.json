 {
    "plugins": ["theme-default", "-lunr", "-search", "search-plus", "addcssjs", "medium-zoom"],
    "pluginsConfig": {
        "fontsettings": {
            "theme":  "night",
            "family": "sans"
        },
        "addcssjs": {
            "css": [
              "build-files/styles/website.css"
            ]
        }
    },
    "links": {
        "sharing": {
            "google": false,
            "facebook": false,
            "twitter": false,
            "weibo": false,
            "all": false
        }
    },
    "medium-zoom": {
      "margin": 24,
      "background": "#363b40",
      "scrollOffset": 0
    },
    "pdf": {
        "pageNumbers": false,
        "fontSize": 12,
        "paperSize": "a4",
        "margin": {
            "right": 62,
            "left": 62,
            "top": 36,
            "bottom": 36
        },
        "headerTemplate": null,
        "footerTemplate": null
    }
}
