 {
    "plugins": ["theme-default", "-lunr", "-search", "search-plus", "addcssjs", "hints", "codetabs", "api", "page-ref", "simpletabs"],
    "pluginsConfig": {
        "fontsettings": {
            "theme":  "night",
            "family": "sans"
        },
        "addcssjs": {
            "css": ["styles/fonts.css"]
        }
    },
    "links": {
        "sharing": {
            "google": false,
            "facebook": false,
            "twitter": false,
            "weibo": false,
